{
  "name": "food-trace-system",
  "version": "1.0.0",
  "description": "食品溯源系统",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:unit": "jest --config=config/test/jest.config.js --testPathPattern='tests/unit'",
    "test:integration": "jest --config=config/test/jest.config.js --testPathPattern='tests/integration'",
    "test:e2e": "playwright test --config=config/test/playwright.config.js",
    "test:e2e:ui": "playwright test --ui --config=config/test/playwright.config.js",
    "test:unit:run": "npm run test:unit && npm run lint",
    "test:integration:run": "npm run test:integration",
    "test:all": "node tests/run-all-tests.js",
    "test:resource": "node validation/scripts/check-resource.js",
    "test:ui": "node validation/scripts/check-ui.js",
    "test:pages": "node validation/scripts/check-page-transitions.js",
    "test:func": "node validation/scripts/check-functionality.js",
    "test:html": "node validation/scripts/check-html-structure.js",
    "test:buttons": "node validation/scripts/check-buttons.js",
    "fix:html": "node validation/scripts/fix-html-structure.js",
    "fix:buttons": "node validation/scripts/check-buttons.js --fix",
    "fix:buttons:check": "npm run validate:buttons && npm run fix:buttons && npm run validate:buttons:after-fix",
    "fix:buttons:report": "node ../scripts/validation/scripts/generate-button-issues.js",
    "fix:specific-buttons": "node ../scripts/utils/button-fixes/fix-specific-buttons.js",
    "fix:page-transitions": "node ../scripts/utils/button-fixes/fix-page-transitions.js",
    "fix:button-navigation": "node ../scripts/utils/button-fixes/fix-button-navigation.js",
    "fix:button-navigation:advanced": "node ../scripts/utils/button-fixes/fix-button-navigation.js --advanced",
    "test:button-navigation:advanced": "node ../scripts/utils/button-fixes/test-button-navigation.js --detailed",
    "fix:all-buttons": "npm run fix:buttons && npm run fix:specific-buttons && npm run fix:button-navigation:advanced",
    "validate:buttons:after-fix": "node validation/scripts/validate-button-improvements.js --output=validation/reports/button_validation_after_fix.json",
    "validate:buttons:compare": "node ../scripts/validation/scripts/validate-fixed-buttons.js",
    "validate:farming:buttons": "node ../scripts/validation/scripts/validate-farming-buttons.js",
    "fix:farming:buttons": "node ../scripts/validation/scripts/fix-farming-buttons.js",
    "fix:farming:all": "npm run fix:farming:buttons && npm run validate:farming:buttons",
    "start": "npx serve -l 8080",
    "dev": "serve -l 8081",
    "build": "webpack --config config/build/webpack.config.js",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "build:css": "postcss styles/index.css -o assets/css/main.css",
    "deploy": "npm run build:css && npm run deploy:assets",
    "deploy:assets": "node deploy-assets.js",
    "deploy:simple": "surge ./ food-trace-prototype.surge.sh",
    "validate": "node ../scripts/validation/scripts/run-all-tests.js",
    "validate:buttons": "node validation/scripts/validate-button-improvements.js",
    "validate:functionality": "node validation/scripts/check-functionality.js",
    "validate:navigation": "node validation/scripts/check-page-transitions.js",
    "validate:page-transitions": "node validation/scripts/check-page-transitions.js --detailed-report",
    "validate:resources": "node validation/scripts/check-resource.js",
    "validate:ui": "node validation/scripts/check-ui.js",
    "validate:farming": "node validation/scripts/run-farming-tests.js",
    "validate:migrate": "node ../scripts/validation/migrate-validation-scripts.js",
    "setup:validation": "powershell -ExecutionPolicy Bypass -File ../scripts/validation/setup.ps1",
    "setup:git": "powershell -ExecutionPolicy Bypass -File ../scripts/dev/git/setup-git-scripts.ps1",
    "setup:debug": "powershell -ExecutionPolicy Bypass -File ../scripts/dev/setup-debug-tools.ps1",
    "setup:all": "npm run setup:validation && npm run setup:git && npm run setup:debug",
    "generate:navigation-report": "node validation/scripts/create-navigation-report.js",
    "test:button-navigation": "node ../scripts/utils/button-fixes/test-button-navigation.js",
    "fix:resources": "node ../scripts/utils/resource-fixes/fix-resources.js",
    "fix:specific-resources": "node ../scripts/utils/resource-fixes/fix-specific-resources.js",
    "test:all-resources": "npm run fix:specific-resources && npm run test:resource",
    "mock-server": "node ../scripts/validation/scripts/run-all-tests.js",
    "test:performance": "node src/network/run-device-performance-test.js --mock"
  },
  "keywords": [
    "food",
    "traceability",
    "supply-chain"
  ],
  "author": "食品溯源系统开发团队",
  "license": "ISC",
  "dependencies": {
    "@playwright/test": "^1.35.1",
    "@zxing/library": "^0.20.0",
    "axios": "^1.6.7",
    "chart.js": "^4.4.1",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^3.3.1",
    "dexie": "^3.2.4",
    "exceljs": "^4.4.0",
    "html5-qrcode": "^2.3.8",
    "jsbarcode": "^3.11.6",
    "jsqr": "^1.4.0",
    "lodash": "^4.17.21",
    "marked": "^11.2.0",
    "pako": "^2.1.0",
    "papaparse": "^5.4.1",
    "pdfjs-dist": "^4.0.379",
    "puppeteer": "^24.6.0",
    "qrcode": "^1.5.3",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.27.1",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "browser-sync": "^3.0.4",
    "chai": "^5.2.0",
    "core-js": "^3.42.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "^8.1.1",
    "sinon": "^20.0.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j4xie/my-prototype-logistics.git"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/j4xie/my-prototype-logistics/issues"
  },
  "homepage": "https://github.com/j4xie/my-prototype-logistics#readme"
}
