          if (kw) {
            const moduleMatch = m.name.toLowerCase().includes(kw);
            filteredApis = filteredApis.filter(api =>
              moduleMatch || api.path.toLowerCase().includes(kw) || api.desc.toLowerCase().includes(kw)
            );
          }

          return { ...m, filteredApis, hasMatch: filteredApis.length > 0 };
        }).filter(m => m.hasMatch);
      } else {
        filtered = filtered.map(m => ({ ...m, filteredApis: m.apis }));
      }

      // Calculate total API count
      const totalApis = filtered.reduce((sum, m) => sum + m.filteredApis.length, 0);
      updateResultCount(filtered.length, totalApis);

      grid.innerHTML = filtered.map(module => `
        <div class="module-card expanded" data-module-id="${module.id}">
          <div class="module-header" onclick="toggleModule('${module.id}')">
            <div class="module-title">
              <span class="module-icon">${module.icon}</span>
              <span>${module.name}</span>
              ${module.usageGuide ? `<button class="guide-btn" onclick="showGuide('${module.id}')" title="æŸ¥çœ‹ä½¿ç”¨æŒ‡å—">ğŸ“– æŒ‡å—</button>` : ''}
            </div>
            <div>
              <span class="module-badge">${module.filteredApis.length} APIs</span>
              <span class="toggle-icon" style="margin-left: 8px;">â–¼</span>
            </div>
          </div>
          <div class="module-content">
            <ul class="api-list">
              ${module.filteredApis.map(api => `
                <li class="api-item">
                  <span class="api-method ${api.method.toLowerCase()}">${api.method}</span>
                  <span class="api-path">${api.path}</span>
                  <button class="copy-btn" onclick="event.stopPropagation(); copyToClipboard('${api.path}')">å¤åˆ¶</button>
                  <span class="api-desc">${api.desc}</span>
                </li>
              `).join('')}
            </ul>
          </div>
        </div>
      `).join('');
    }

    function renderRoles() {
      const grid = document.getElementById('rolesGrid');
      grid.innerHTML = ROLES.map(role => `
        <div class="role-card" onclick="showRoleGuide('${role.id}')" title="ç‚¹å‡»æŸ¥çœ‹ä½¿ç”¨æŒ‡å—">
          <div class="role-icon">${role.icon}</div>
          <div class="role-name">${role.name}</div>
          <div class="role-screens">${role.screens} é¡µé¢</div>
          <div class="role-desc">${role.desc}</div>
        </div>
      `).join('');
    }

    function toggleModule(moduleId) {
      const card = document.querySelector(`[data-module-id="${moduleId}"]`);
      const content = card.querySelector('.module-content');
      card.classList.toggle('expanded');
      content.classList.toggle('collapsed');
    }

    function toggleAllModules(expand) {
      document.querySelectorAll('.module-card').forEach(card => {
        const content = card.querySelector('.module-content');
        if (expand) {
          card.classList.add('expanded');
          content.classList.remove('collapsed');
        } else {
          card.classList.remove('expanded');
          content.classList.add('collapsed');
        }
      });
    }

    // ==================== STATS CALCULATION ====================
    function calculateAndUpdateStats() {
      // è®¡ç®—åŠŸèƒ½æ¨¡å—æ•°é‡
      const totalModules = MODULES.length;

      // è®¡ç®— API æ€»æ•°
      const totalApis = MODULES.reduce((sum, m) => sum + (m.apiCount || 0), 0);

      // è®¡ç®—å‰ç«¯é¡µé¢æ•°é‡ (ä» ROLES ä¸­çš„ screens å­—æ®µ)
      const totalScreens = ROLES.reduce((sum, r) => sum + (r.screens || 0), 0);

      // è®¡ç®— Controller æ•°é‡ (å»é‡)
      const allControllers = new Set();
      MODULES.forEach(m => {
        if (m.controllers) {
          m.controllers.forEach(c => allControllers.add(c));
        }
      });
      const totalControllers = allControllers.size;

      // ç”¨æˆ·è§’è‰²æ•°é‡
      const totalRoles = ROLES.length;

      // æ›´æ–° DOM - é¡¶éƒ¨ç»Ÿè®¡æ 
      document.getElementById('totalModules').textContent = totalModules;
      document.getElementById('totalApis').textContent = totalApis;
      document.getElementById('totalScreens').textContent = totalScreens;
      document.getElementById('totalControllers').textContent = totalControllers;
      document.getElementById('totalRoles').textContent = totalRoles;

      // æ›´æ–° Footer
      document.getElementById('footerControllers').textContent = totalControllers;
      document.getElementById('footerScreens').textContent = totalScreens;

      console.log('Stats calculated:', { totalModules, totalApis, totalScreens, totalControllers, totalRoles });
    }

    // ==================== INIT ====================
    document.addEventListener('DOMContentLoaded', () => {
      renderModules();
      renderRoles();
      calculateAndUpdateStats(); // åŠ¨æ€è®¡ç®—ç»Ÿè®¡æ•°æ®

      // Helper to get current filter values
      function getFilters() {
        return {
          category: document.querySelector('.filter-btn.active').dataset.filter,
          keyword: document.getElementById('searchInput').value.trim(),
          method: document.getElementById('methodFilter').value
        };
      }

      // Debounced search function
      const debouncedRender = debounce(() => {
        const { category, keyword, method } = getFilters();
        renderModules(category, keyword, method);
      }, 300);

      // Search with debounce
      document.getElementById('searchInput').addEventListener('input', debouncedRender);

      // Method Filter
      document.getElementById('methodFilter').addEventListener('change', () => {
        const { category, keyword, method } = getFilters();
        renderModules(category, keyword, method);
      });

      // Category Filter
      document.querySelectorAll('.filter-btn[data-filter]').forEach(btn => {
        btn.addEventListener('click', (e) => {
          document.querySelectorAll('.filter-btn[data-filter]').forEach(b => b.classList.remove('active'));
          e.target.classList.add('active');
          const { category, keyword, method } = getFilters();
          renderModules(category, keyword, method);
        });
      });

      // Expand All
      let allExpanded = true;
      document.getElementById('expandAll').addEventListener('click', (e) => {
        allExpanded = !allExpanded;
        toggleAllModules(allExpanded);
        e.target.textContent = allExpanded ? 'å…¨éƒ¨æ”¶èµ·' : 'å…¨éƒ¨å±•å¼€';
      });
    });
  </script>
</body>
</html>
