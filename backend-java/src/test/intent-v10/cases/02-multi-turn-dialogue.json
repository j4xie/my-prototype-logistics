{
  "category": "multi_turn_dialogue",
  "description": "多轮对话与参数收集测试",
  "testCases": [
    {
      "id": "M001",
      "input": "我要领一些原料",
      "expectedIntent": "MATERIAL_BATCH_USE",
      "description": "渐进式参数收集 - 原料领用申请",
      "expectClarification": true,
      "clarificationFields": ["materialType", "quantity"]
    },
    {
      "id": "M002",
      "input": "帮我安排一个质检",
      "expectedIntent": "QUALITY_CHECK_EXECUTE",
      "description": "渐进式参数收集 - 质检任务创建",
      "expectClarification": true,
      "clarificationFields": ["checkType", "batchNumber"]
    },
    {
      "id": "M003",
      "input": "我要创建一张发货单",
      "expectedIntent": "SHIPMENT_CREATE",
      "description": "渐进式参数收集 - 发货单创建",
      "expectClarification": true,
      "clarificationFields": ["customer", "product", "quantity"]
    },
    {
      "id": "M004",
      "input": "帮我申请请假",
      "expectedIntent": "LEAVE_REQUEST",
      "description": "渐进式参数收集 - 员工请假申请",
      "expectClarification": true,
      "clarificationFields": ["leaveType", "startDate", "endDate"]
    },
    {
      "id": "M005",
      "input": "有台设备坏了",
      "expectedIntent": "EQUIPMENT_REPAIR",
      "description": "渐进式参数收集 - 设备报修",
      "expectClarification": true,
      "clarificationFields": ["equipmentId", "faultDescription"]
    },
    {
      "id": "M006",
      "input": "我想调拨一些库存",
      "expectedIntent": "MATERIAL_TRANSFER",
      "description": "渐进式参数收集 - 库存调拨",
      "expectClarification": true,
      "clarificationFields": ["sourceWarehouse", "targetWarehouse", "material", "quantity"]
    },
    {
      "id": "M007",
      "input": "帮我录入供应商评价",
      "expectedIntent": "SUPPLIER_EVALUATION",
      "description": "渐进式参数收集 - 供应商评价",
      "expectClarification": true,
      "clarificationFields": ["supplier", "evaluationDimensions"]
    },
    {
      "id": "M008",
      "input": "需要调整一下明天的排产",
      "expectedIntent": "SCHEDULE_UPDATE",
      "description": "渐进式参数收集 - 排产计划调整",
      "expectClarification": true,
      "clarificationFields": ["adjustmentContent", "quantity"]
    },
    {
      "id": "M009",
      "input": "有个客户投诉",
      "expectedIntent": "COMPLAINT_REGISTER",
      "description": "渐进式参数收集 - 客户投诉登记",
      "expectClarification": true,
      "clarificationFields": ["customerName", "complaintContent"]
    },
    {
      "id": "M010",
      "input": "帮我算一下成本",
      "expectedIntent": "COST_ANALYSIS",
      "description": "渐进式参数收集 - 成本核算请求",
      "expectClarification": true,
      "clarificationFields": ["costObject", "costPeriod"]
    },
    {
      "id": "M011",
      "input": "帮我追溯一下",
      "expectedIntent": "TRACE_FULL",
      "description": "渐进式参数收集 - 批次追溯",
      "expectClarification": true,
      "clarificationFields": ["traceDirection", "batchNumber"]
    },
    {
      "id": "M012",
      "input": "录入绩效考核",
      "expectedIntent": "PERFORMANCE_RECORD",
      "description": "渐进式参数收集 - 绩效考核录入",
      "expectClarification": true,
      "clarificationFields": ["targetEmployee", "evaluationPeriod"]
    },
    {
      "id": "M013",
      "input": "这批原料要退货",
      "expectedIntent": "MATERIAL_RETURN",
      "description": "渐进式参数收集 - 原料退货",
      "expectClarification": true,
      "clarificationFields": ["batchNumber", "returnReason"]
    },
    {
      "id": "M014",
      "input": "想调整一下产品价格",
      "expectedIntent": "PRICE_ADJUSTMENT",
      "description": "渐进式参数收集 - 价格调整申请",
      "expectClarification": true,
      "clarificationFields": ["product", "adjustmentMethod", "effectiveDate"]
    },
    {
      "id": "M015",
      "input": "修改一下工艺参数",
      "expectedIntent": "PROCESS_PARAMETER_UPDATE",
      "description": "渐进式参数收集 - 工艺参数修改",
      "expectClarification": true,
      "clarificationFields": ["processStep", "parameterName", "newValue"]
    },
    {
      "id": "M016",
      "input": "我想看看这批货的情况",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - '情况'含义模糊",
      "expectClarification": true,
      "clarificationFields": ["situationType"]
    },
    {
      "id": "M017",
      "input": "帮我处理一下",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - '处理'含义模糊",
      "expectClarification": true,
      "clarificationFields": ["processType", "targetObject"]
    },
    {
      "id": "M018",
      "input": "查一下张三的记录",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - '记录'类型不明",
      "expectClarification": true,
      "clarificationFields": ["recordType"]
    },
    {
      "id": "M019",
      "input": "更新一下状态",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - 更新什么的状态",
      "expectClarification": true,
      "clarificationFields": ["objectType", "objectId"]
    },
    {
      "id": "M020",
      "input": "数量不太对",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - 什么数量",
      "expectClarification": true,
      "clarificationFields": ["quantityType", "targetObject"]
    },
    {
      "id": "M021",
      "input": "安排一下明天的工作",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - 安排什么工作",
      "expectClarification": true,
      "clarificationFields": ["workType"]
    },
    {
      "id": "M022",
      "input": "导出一份报表",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - 什么报表",
      "expectClarification": true,
      "clarificationFields": ["reportType"]
    },
    {
      "id": "M023",
      "input": "检查一下有没有问题",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - 检查什么",
      "expectClarification": true,
      "clarificationFields": ["checkTarget"]
    },
    {
      "id": "M024",
      "input": "这个要改一下",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - 改什么",
      "expectClarification": true,
      "clarificationFields": ["modifyTarget", "modifyContent"]
    },
    {
      "id": "M025",
      "input": "统计一下最近的数据",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - 什么数据，时间范围是？",
      "expectClarification": true,
      "clarificationFields": ["dataType", "timeRange"]
    },
    {
      "id": "M026",
      "input": "帮我查一下那个订单",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - 哪个订单",
      "expectClarification": true,
      "clarificationFields": ["orderId"]
    },
    {
      "id": "M027",
      "input": "预警好像有问题",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - 什么预警",
      "expectClarification": true,
      "clarificationFields": ["alertType"]
    },
    {
      "id": "M028",
      "input": "对比一下两个批次",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - 对比什么维度，批次号是？",
      "expectClarification": true,
      "clarificationFields": ["compareDimension", "batchNumbers"]
    },
    {
      "id": "M029",
      "input": "这个供应商怎么样",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - 查询什么信息",
      "expectClarification": true,
      "clarificationFields": ["supplierInfo", "supplierId"]
    },
    {
      "id": "M030",
      "input": "分析一下原因",
      "expectedIntent": "NEED_CLARIFICATION",
      "description": "意图消歧 - 分析什么问题的原因",
      "expectClarification": true,
      "clarificationFields": ["problemDescription"]
    }
  ]
}
