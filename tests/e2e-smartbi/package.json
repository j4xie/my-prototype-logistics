{
  "name": "smartbi-e2e-tests",
  "version": "1.0.0",
  "description": "SmartBI E2E 自动化测试工具 - Chrome 自动化调试",
  "scripts": {
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:ui": "npx playwright test --ui",
    "ralph-loop": "npx ts-node ralph-loop.ts",
    "ralph-loop:headed": "HEADED=true npx ts-node ralph-loop.ts",
    "report": "npx playwright show-report",
    "install-browsers": "npx playwright install chromium"
  },
  "dependencies": {
    "@playwright/test": "^1.58.2",
    "dotenv": "^16.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0"
  }
}
