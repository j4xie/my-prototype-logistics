# SmartBI Service Configuration

# LLM Configuration - Text Models
LLM_API_KEY=your-api-key-here
LLM_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
LLM_MODEL=qwen-plus

# LLM Configuration - Vision Model (for structure detection)
LLM_VL_MODEL=qwen-vl-max
LLM_VL_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1

# LLM Configuration - Fast Model (for quick operations)
LLM_FAST_MODEL=qwen-turbo

# LLM Configuration - Reasoning Model (for complex decisions)
LLM_REASONING_MODEL=qwq-32b

# Zero-Code Configuration
STRUCTURE_DETECTION_CONFIDENCE_THRESHOLD=0.7
SEMANTIC_MAPPING_CONFIDENCE_THRESHOLD=0.8
ENABLE_MULTI_MODEL_ENHANCEMENT=true
MAX_SELF_CORRECTION_ROUNDS=3

# Service Configuration
DEBUG=false
PORT=8083
HOST=0.0.0.0

# CORS Configuration (comma-separated origins)
# CORS_ORIGINS=["http://localhost:3000","http://localhost:10010"]

# File Upload Configuration
MAX_FILE_SIZE_MB=50

# Cache Configuration
CACHE_TTL_SECONDS=3600
SCHEMA_CACHE_ENABLED=true
