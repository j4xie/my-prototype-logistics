# 数据库配置 - 需要在Vercel中设置
DATABASE_URL="mysql://user:password@host:port/database"

# JWT配置 - 需要在Vercel中设置
JWT_SECRET="your-super-secret-jwt-key-here"
JWT_EXPIRES_IN="7d"

# CORS配置 - 需要在Vercel中设置
CORS_ORIGIN="https://your-frontend-domain.vercel.app"

# 应用配置
PORT=3001
NODE_ENV="production"

# Prisma配置
PRISMA_GENERATE_DATAPROXY="true"
